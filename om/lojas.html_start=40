<script type="text/javascript">
/*<![CDATA[*/

//validate date function
function isDate(txtDate){
    var reg = /^(\d{4})([\/-])(\d{1,2})\2(\d{1,2})$/;
    return reg.test(txtDate);
}

//validate startdate and enddate before submit form
function validateDateRange(obj){
    if(obj.id == 'sdate_162' || obj.id == 'edate_162'){
        var sDate = $('jak2filter162').getElement('#sdate_162').get('value');
        var eDate = $('jak2filter162').getElement('#edate_162').get('value');
        if(sDate != '' && eDate != ''){
            if(isDate(sDate) && isDate(eDate)){
                obj.removeClass('date-error');
                $('jak2filter-form-162').fireEvent('submit');
            }
            else{
                obj.addClass('date-error');
            }
        }
    }
    else{
        $('jak2filter-form-162').fireEvent('submit');
    }
}

do_nothing( function(){
	if($('jak2filter162').getElement('#category_id')){
		jak2DisplayExtraFields(162, $('jak2filter162').getElement('#category_id'), 0);
	}

	
		$('jak2filter-form-162').addEvent('submit', function() {
		$('jak2filter-form-162').submit();
	});
	});
/*]]>*/
</script></div></div>
